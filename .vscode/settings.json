{
   "files.exclude": {
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.github": true,
      "**/.gitp*": true,
      "**/.hg": true,
      "**/.svn": true,
      "**/.vscode": true,
      "**/core.Microsoft*": true,
      "**/core.mongo*": true,
      "**/core.python*": true,
      "**/CVS": true
   },
   "files.autoSave": "off",
   "workbench.colorTheme": "Visual Studio Dark",
   "terminal.integrated.gpuAcceleration": "canvas",
   "notebook.defaultFormatter": "ms-python.debugpy",
   "editor.defaultFormatter": "ms-python.autopep8",
   "editor.defaultFoldingRangeProvider": "ms-python.autopep8",
   "autopep8.cwd": "${currytales}",
   "[python]": {
      "editor.defaultFormatter": "KylinIDETeam.python"
   },
}